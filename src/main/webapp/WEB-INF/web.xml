<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>FrontServlet</servlet-name>
    <servlet-class>com.example.FrontServlet</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>

  <servlet>
    <servlet-name>HrServlet</servlet-name>
    <servlet-class>com.example.HrServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet>
    <servlet-name>ProjectServlet</servlet-name>
    <servlet-class>com.example.ProjectServlet</servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>FrontServlet</servlet-name>
    <url-pattern>/FrontServlet/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>HrServlet</servlet-name>
    <url-pattern>/HrServlet/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ProjectServlet</servlet-name>
    <url-pattern>/ProjectServlet/*</url-pattern>
  </servlet-mapping>

</web-app>
